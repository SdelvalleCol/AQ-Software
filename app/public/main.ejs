        explicabo officia voluptatem accusantium atque nostrum repudiandae harum a vero totam quam labore.
      </div>
    </div>
  </div>

  <!--Formulario-->
  <div class="formulario">
    <!--Titulo-->
    <div class="container">
      <div class="row">
        <div class="col-md-6 mx-auto" style="text-align: center;">
          <h1>Contactanos</h1>
        </div>
      </div>
    </div>
    <!--Información contenedor-->
    <div class="row formulario_contenedor">
      <div class="col-md-6">
        <img src="./recursos/img/gmail.png" class="img" alt="" srcset="">
      </div>
      <div class="col-md-6 formulario_contenedor">
        <input type="text" class="input_form ">
        <input type="text" class="input_form">
        <input type="text" class="input_form">
